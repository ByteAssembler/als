<script defer is:inline>
    const banner = document.getElementById("welcome-banner");

    if (banner && !sessionStorage.getItem("welcomeBannerShown")) {
        banner.classList.remove("invisible");
        // Warten auf das Ende aller Animationen (4 Sekunden: 2s Delay + 1s Animation + 1s Puffer)
        setTimeout(() => {
            // Fade-out Animation starten
            banner.classList.add("fade-out");

            // Nach der Fade-out Animation (1s) das Element komplett entfernen
            setTimeout(() => {
                // banner.classList.add("hidden");
                // Remove from dom
                banner.remove();
            }, 1000);
        }, 4000);
        sessionStorage.setItem("welcomeBannerShown", "true");
    }
</script>
